<!DOCTYPE html>
<html>
<head>
  <title>Winenotes</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header>
    <% if current_user %>
      Signed in as <%= current_user.email %>
      <%= link_to 'Sign Out', sign_out_path %>
    <% end %>
  </header>

  <%= render 'layouts/messages' %>
  <%= yield %>

</body>
</html>
