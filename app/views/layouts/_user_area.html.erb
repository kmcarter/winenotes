<li>
  Welcome
  <% if current_user.name != "Guest" %>
    <%= link_to(image_tag(current_user.gravatar_url(size: 20)), user_path(current_user), class: "avatar") %>
  <% end %> 
  <%= current_user.name %>
</li>
<% user_links.each do |link| %>
  <li><%= link_to link.name, link.target %></li>
<% end %>